{
  "name": "@scribest/neuvue",
  "description": "Vue component library implementing a Neumorphic design.",
  "version": "0.0.2",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scribest/neuvue.git"
  },
  "keywords": [
    "neuvue",
    "neumorphism",
    "neumorphic",
    "design",
    "ui framework",
    "component framework",
    "ui library",
    "component library",
    "vue framework",
    "vue",
    "vuejs"
  ],
  "author": "Gârleanu Alexandru-Ștefan <garleanualexandru@yahoo.com>",
  "main": "dist/neuvue.ssr.js",
  "browser": "dist/neuvue.esm.js",
  "module": "dist/neuvue.esm.js",
  "unpkg": "dist/neuvue.min.js",
  "types": "neuvue.d.ts",
  "files": [
    "dist/*",
    "neuvue.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "consistent-shading": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.35",
    "rollup": "^2.26.5",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-ts": "^1.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "typescript": "^4.0.3",
    "vue": "^3.0.0-0",
    "vue-class-component": "^8.0.0-0",
    "vue-jest": "^5.0.0-0"
  },
  "peerDependencies": {
    "vue": "^3.0.0-0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
